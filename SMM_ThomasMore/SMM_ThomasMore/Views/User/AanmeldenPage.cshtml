@model SMM_ThomasMore.Models.UserVM
@{
    ViewBag.Title = "Aanmelden";
}

<h2>Aanmelden</h2>


@using (Html.BeginForm())
{
@Html.AntiForgeryToken()

<ul class="list-unstyled">
    <li>
        @Html.EditorFor(model => model.username, new { htmlAttributes = new { placeholder = "Gebruikersnaam" } })
    </li>
    <li>
        @Html.EditorFor(model => model.wachtwoord, new { htmlAttributes = new { placeholder = "Wachtwoord" } })
    </li>
    <li>
        <input type="submit" value="Meld aan">
    </li>
    <li>
        <button>Login met Facebook</button>
    </li>
    <li>
        <button>Login met Google+</button>
    </li>
</ul>
}





