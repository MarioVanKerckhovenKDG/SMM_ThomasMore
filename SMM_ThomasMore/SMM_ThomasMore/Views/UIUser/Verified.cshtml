@model SMM_ThomasMore.Models.VerifyVM
@{
    ViewBag.Title = "Verified";
    var userid = Request.Url.Segments.Last();
}


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    @Html.EditorFor(model => model.id, new { htmlAttributes = new { @Value=@userid.ToString() , @class = "form-control" } })
    <input type="submit" value="Voltooi de registratie">
}

